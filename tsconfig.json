{
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "outDir": "out",
    "lib": [
      "es6",
      "es2021"
    ],
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true,
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "test.js"
  ]
}
